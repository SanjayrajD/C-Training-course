#include<iostream>
using namespace std;
int main()
{
    int a,b;
    b=5;
    cin>>a;
    for(int i=3;i<=a;i++)
    {
        cout<<" "<<" "<<" "<<"*";
    }
    cout<<endl;
     for(int j=1;j<=a;j++)
     {
         cout<<" "<<"*";
     }
     cout<<endl;
     for(int k=1;k<=b;k++)
     {
         cout<<"*"<<" ";
     }
}

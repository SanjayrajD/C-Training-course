#include<iostream>
using namespace std;
int main()
{
    int x,y;
    cin>>x>>y;
    int a[x][y];
    for(int i=0;i<x;i++)
    {
       for(int j=0;j<y;j++) 
    {
        cin>>a[i][j];
    }
    }
     for(int i=0;i<x;i++)
    {
       for(int j=0;j<y;j++)
       {
           if(a[0][0]<a[i][j])
           {
               a[0][0]=a[i][j];
           }
       }
        cout<<a[0][0]<<endl;
         a[0][0]=0;
      
}
}

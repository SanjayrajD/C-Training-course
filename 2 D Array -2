#include<iostream>
using namespace std;
int main()
{
    int r;
    cin>>r;
    int a[r][r];
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<r;j++)
        {
            cin>>a[i][j];
        }
    }
    cout<<"Transpose matrix is :"<<endl;
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<r;j++)
        {
            cout<<a[j][i]<<" ";
            if(j==r-1)
            {
                cout<<endl;
            }
        }
}}

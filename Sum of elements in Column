#include<iostream>
using namespace std;
 int main()
 {
     int x,y,z=0;
     cin>>x>>y;
     int a[x][y];
     for(int i=0;i<x;i++)
     {
         for(int j=0;j<y;j++)
         {
             cin>>a[i][j];
         }
     }
     for(int i=0;i<y;i++)
     {
         for(int j=0;j<x;j++)
         {
           z=z+a[j][i];  
         }
         cout<<z<<endl;
         z=0;
     }
 }
